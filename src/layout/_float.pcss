
@responsive {

  .clearfix:before,
  .clearfix:after {
    content: " ";
    display: table;
  }

  .clearfix:after {
    clear: both;
  }

  .clear-both {
    clear: both;
  }

  .clear-left {
    clear: left;
  }

  .clear-right {
    clear: right;
  }

  .float-left {
    float: left;
  }

  .float-right {
    float: right;
  }

}
